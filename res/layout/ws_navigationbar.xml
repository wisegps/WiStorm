<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    

    <com.wicare.wistorm.ui.WNavigationBar
        android:id="@+id/wn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
  
    
        <com.wicare.wistorm.ui.WWrapContentHeightViewPager
	        android:id="@+id/viewpager"  
		    android:layout_width="match_parent"  
		    android:layout_height="wrap_content"
	        android:fadeScrollbars="false"
	        android:fadingEdge="none" >
    	</com.wicare.wistorm.ui.WWrapContentHeightViewPager>
        

    
	    <LinearLayout
	        android:id="@+id/llyt_circle_page"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_gravity="center_horizontal" >
	
	       <ImageView
	           android:id="@+id/iv_circle_page_0"
	           android:layout_width="wrap_content"
	           android:layout_height="wrap_content"
	           android:layout_marginRight="5dp"
	           android:src="@drawable/ws_img_circle_dot_blue" />
	
	       <ImageView
	           android:id="@+id/iv_circle_page_1"
	           android:layout_width="wrap_content"
	           android:layout_height="wrap_content"
	           android:layout_marginLeft="5dp"
	           android:src="@drawable/ws_img_circle_dot_blue_light" />
	    </LinearLayout> 
	        
       
    </com.wicare.wistorm.ui.WNavigationBar>
    
    
    
</LinearLayout>
